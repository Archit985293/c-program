#include <stdio.h>

int main() {
    int n, k;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[200];
    printf("Enter array elements:\n");
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter subarray size k: ");
    scanf("%d", &k);

    if(k > n || k <= 0) {
        printf("Invalid subarray size.\n");
        return 0;
    }

    for(int i = 0; i <= n - k; i++) {
        int found = 0; // flag for first negative
        for(int j = 0; j < k; j++) {
            if(arr[i + j] < 0) {
                printf("%d", arr[i + j]);
                found = 1;
                break; // stop at first negative
            }
        }
        if(!found) printf("0");
        if(i != n - k) printf(" ");
    }

    printf("\n");
    return 0;
}

#include <stdio.h>

int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[200];
    printf("Enter array elements (size n, numbers from 1 to n-1, one repeated):\n");
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    int xorArr = 0;
    int xorRange = 0;

    // XOR of all array elements
    for(int i = 0; i < n; i++)
        xorArr ^= arr[i];

    // XOR of numbers from 1 to n-1
    for(int i = 1; i <= n - 1; i++)
        xorRange ^= i;

    int repeated = xorArr ^ xorRange;

    printf("%d\n", repeated);

    return 0;
}

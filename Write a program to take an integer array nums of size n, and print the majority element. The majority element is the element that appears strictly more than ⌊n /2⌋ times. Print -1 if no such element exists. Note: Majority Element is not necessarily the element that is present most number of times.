#include <stdio.h>

int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[200];
    printf("Enter array elements:\n");
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    // Boyer-Moore Voting Algorithm
    int candidate = arr[0];
    int count = 1;

    for(int i = 1; i < n; i++) {
        if(arr[i] == candidate)
            count++;
        else
            count--;

        if(count == 0) {
            candidate = arr[i];
            count = 1;
        }
    }

    // Verify if candidate is actually majority
    count = 0;
    for(int i = 0; i < n; i++) {
        if(arr[i] == candidate)
            count++;
    }

    if(count > n / 2)
        printf("%d\n", candidate);
    else
        printf("-1\n");

    return 0;
}

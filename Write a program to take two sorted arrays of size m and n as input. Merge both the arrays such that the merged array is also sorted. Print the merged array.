#include <stdio.h>

int main() {
    int m, n;
    printf("Enter size of first array: ");
    scanf("%d", &m);
    int arr1[200];
    printf("Enter elements of first sorted array:\n");
    for(int i = 0; i < m; i++)
        scanf("%d", &arr1[i]);

    printf("Enter size of second array: ");
    scanf("%d", &n);
    int arr2[200];
    printf("Enter elements of second sorted array:\n");
    for(int i = 0; i < n; i++)
        scanf("%d", &arr2[i]);

    int merged[400];
    int i = 0, j = 0, k = 0;

    // Merge arrays
    while(i < m && j < n) {
        if(arr1[i] <= arr2[j])
            merged[k++] = arr1[i++];
        else
            merged[k++] = arr2[j++];
    }

    // Copy remaining elements
    while(i < m) merged[k++] = arr1[i++];
    while(j < n) merged[k++] = arr2[j++];

    // Print merged array
    for(int l = 0; l < k; l++) {
        printf("%d", merged[l]);
        if(l != k - 1) printf(" ");
    }
    printf("\n");

    return 0;
}

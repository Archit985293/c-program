#include <stdio.h>
#include <string.h>

int main() {
    char s[200], t[200];
    printf("Enter first string: ");
    scanf("%s", s);
    printf("Enter second string: ");
    scanf("%s", t);

    int countS[26] = {0};
    int countT[26] = {0};

    int n1 = strlen(s);
    int n2 = strlen(t);

    if(n1 != n2) {
        printf("Not Anagram\n");
        return 0;
    }

    // Count frequency of each character
    for(int i = 0; i < n1; i++) {
        countS[s[i]-'a']++;
        countT[t[i]-'a']++;
    }

    // Compare frequency arrays
    for(int i = 0; i < 26; i++) {
        if(countS[i] != countT[i]) {
            printf("Not Anagram\n");
            return 0;
        }
    }

    printf("Anagram\n");
    return 0;
}

#include <stdio.h>
int main()
{
    int num1,num2,num3,maxnum;
    printf("enter three numbers num1,num2,num3");
    scanf("%d %d %d",&num1,&num2,&num3);
    
    if(num1 >= num2 && num1 >= num3)
        maxnum=num1;
        
    else if(num2 >= num3 && num2 >= num1)
        maxnum=num2;
        
    else
        maxnum=num3;
    printf("largest number = %d\n",maxnum);
    
    return 0;
    
}

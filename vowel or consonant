#include <stdio.h>
int main()
{
  char alphabet,a,e,i,o,u;
  printf("enter an alphabet");
  scanf("%c",&alphabet);
  
  if(alphabet == 'a' || alphabet == 'e' || alphabet == 'i' || alphabet == 'o' || alphabet == 'u')
    printf("it is a vowel");
  else
    printf("it is a consonant");
    
  return 0;
}
